<interface>
  <requires lib="gtk+" version="3.20"/>
  <object id="window" class="GtkWindow">
    <property name="visible">True</property>
    <property name="can_focus">True</property>
    <property name="resizable">False</property>
    <property name="default_width">360</property>
    <property name="default_height">240</property>
    <child>
      <object class="GtkBox">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="halign">center</property>
        <property name="valign">center</property>
        <property name="spacing">9</property>
        <child>
          <object class="GtkComboBoxText" id="hours">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="active">0</property>
            <items>
              <item id="1" translatable="yes">00</item>
              <item id="2" translatable="yes">01</item>
              <item id="3" translatable="yes">02</item>
              <item id="4" translatable="yes">03</item>
              <item id="5" translatable="yes">04</item>
              <item id="6" translatable="yes">05</item>
              <item id="7" translatable="yes">06</item>
              <item id="8" translatable="yes">07</item>
              <item id="9" translatable="yes">08</item>
              <item id="10" translatable="yes">09</item>
              <item id="11" translatable="yes">10</item>
              <item id="12" translatable="yes">11</item>
              <item id="13" translatable="yes">12</item>
              <item id="14" translatable="yes">13</item>
              <item id="15" translatable="yes">14</item>   
              <item id="16" translatable="yes">15</item>
              <item id="17" translatable="yes">16</item>
              <item id="18" translatable="yes">17</item>
              <item id="19" translatable="yes">18</item>
              <item id="20" translatable="yes">19</item>
              <item id="21" translatable="yes">20</item>
              <item id="22" translatable="yes">21</item>
              <item id="23" translatable="yes">22</item>
              <item id="24" translatable="yes">23</item>
            </items>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">False</property>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkComboBoxText" id="minutes">
            <property name="visible">True</property>
            <property name="can_focus">False</property>
            <property name="active">0</property>
            <items>
              <item id="1" translatable="yes">00</item>
              <item id="2" translatable="yes">10</item>
              <item id="3" translatable="yes">20</item>
              <item id="4" translatable="yes">30</item>
              <item id="5" translatable="yes">40</item>
              <item id="6" translatable="yes">50</item>
            </items>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkSwitch" id="switch">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
          </object>
          <packing>
            <property name="expand">False</property>
            <property name="fill">True</property>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
